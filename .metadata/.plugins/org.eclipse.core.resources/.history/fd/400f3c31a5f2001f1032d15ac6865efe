package sec02.verify.exam05;

public class practice {
	public static void main(String[] args) {
		int[][] array = {
				{95, 86},
				{83, 92, 96},
				{78, 83, 93, 87, 88}
		};
		
		int sum = 0;
		double avg = 0.0;
		
		int count = 0;
		for(int i=0; i<array.length; i++) { // i 값은 순차적으로 0 부터 1씩 증감되며 들어오고 0,1,2 <array.length(3)이므로 조건식 성립이 3번 일어남
			for(int j=0; j<array[i].length; j++) { // i 에는 0,1,2가 들어오며 0번, 1번, 2번 인덱스 값이
				sum += array[i];
				
			}
		}
		
	}
}
