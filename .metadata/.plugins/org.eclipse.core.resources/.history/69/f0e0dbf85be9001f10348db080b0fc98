package sec02.exam04;

public class starpractice {
	public static void main(String[] args) {
		/*
		 2<5
		 8<2
		 6<7
		 0>1
		 102>304
		 h<a
		 z>t
		 
		 banana < banarcle 
		 * */
		for (int i=10; i<15; i++) {
			for (int k=4; k>0+i-10; k--) {
				System.out.print(" ");
			}
			for (int j=1; j<=2*i-1-9; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		System.out.print("끝");    
	}
}
