package sec02.exam04;

public class triangle {
	public static void main(String[] args) {
		for (int i=1; i<=5; i++) {
			
			
			
			
			
			
			for (int b=1; b<=i; b++) {
			System.out.print(" ");
			}
		for (int j=1; j<=9 -2*i; j++) {
			System.out.print("*");
		}
			System.out.println();
		}
	}
}
