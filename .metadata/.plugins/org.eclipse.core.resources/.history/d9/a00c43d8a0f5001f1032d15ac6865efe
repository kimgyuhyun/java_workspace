package sec03.verify.exam03;

public class TestBoard {
	String title;
	String content;
	String writer;
	String date;
	int hitcount;
	
	TestBoard(String title, String content) {
	
	}
	
	TestBoard(String title, String content, String writer) {

	}
	
	TestBoard(String title, String content, String writer, String date) {

	}
	
	TestBoard(String title, String content, String writer, String date, int hitcount) {
		this.title = title;
		this.content = content;
		this.writer = writer;
		this.date = date;
		this.hitcount = hitcount;
	}
}
