package sec02.exam09;

public class practice {
	public static void main(String[] args) {
		int charCode = 'A'; //65
		
		if( (charCode>=65) & (charCode<=90) ) { //charCode가 65보다 크거나 같다면, 90보다 charCode가 작거나 같다면
			System.out.println("대문자군요");
		}
		
		if( (charCode>=97) && (charCode<=122) ) { //charCode가 97보다 크거나 같으면, 122보다 charCode가 작거나 같다면
			System.out.println("소문자군요");
		}
		
		if( !(charCode<48) && !(charCode>57) ) { //48이 charCode보다 더클때, charCode가 57보다 클때
			System.out.println("0~9 숫자군요");
		}
		
		int value = 6;

		if ( (value%2==0) | (value%3==0) ) { //value를 2로 나누고 나머지가 0일때, value를 3으로 나누고 나머지가 0일때
			System.out.println("2 또는 3의 배수군요");
		}
		
		if( (value%2==0) || (value%3==0) ) { //value를 2로 나누고 나머지가 0일때, value를 3으로 나누고 나머지가 0일때
			System.out.println("2 또는 3의 배수군요");
		}
	}
}
